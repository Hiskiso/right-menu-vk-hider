{
    "manifest_version": 3,
    "name": "Скрыть нахуй плашку ебаную",
    "description": "Как и просили",
    "version": "1.0",
    "icons": {
    "128": "128.png"
    },
    "permissions": [
    "unlimitedStorage",
    "notifications",
    "storage", 
    "activeTab", 
    "scripting"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://vk.com/*"],
        "js": ["contentScript.js"]
      }
    ]
    }